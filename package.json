  },
  "homepage": "https://github.com/ebi-uniprot/pepvep-node-service#readme",
  "dependencies": {
    "@types/node": "^10.9.4",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "object.values": "^1.0.4",
    "tunnel": "^0.0.6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
